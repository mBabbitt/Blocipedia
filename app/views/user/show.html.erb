<h1><%= @user.email %>'s profile</h1>
<div>
  <%= form_for @user do |f| %>
      <div class='form-group'>
        <%= radio_button_tag(:role, 'standard') %>
        <%= label_tag(:role, 'standard') %> <br/>
        <%= radio_button_tag(:role, 'premium') %>
        <%= label_tag(:role, 'premium') %> <br/>
     
      </div>
      <div class='form-group'>
        <%= f.submit "Save", class: 'btn btn-success' %>
      </div>
  <% end %>
</div>
